		"@types/node": "^24.6.2",
		"@types/react": "^19.2.0",
		"@types/react-dom": "^19.2.0",
		"dotenv": "^17.2.3",
		"npm-run-all": "^4.1.5",
		"tailwindcss": "^4.1.14",
		"tw-animate-css": "^1.4.0",
		"typescript": "^5.9.3"
	}
